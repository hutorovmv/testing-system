@if (TempData["PartialMessageSuccess"] != null)
{
    <div class="alert alert-success alert-dismissible fade show" role="alert">
        @TempData["PartialMessageSuccess"]
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
    </div>
}
else if (TempData["PartialMessageFailure"] != null)
{
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        @TempData["PartialMessageFailure"]
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
    </div>
}