@model IEnumerable<TestingSystem.Web.Models.TestResultTableModel>

@foreach (var item in Model)
{
    <tr class="text-center">
        <td>
            @Html.ActionLink(item.TestName, "ShowTestDescription", "TestPassing", new { area = "", testId = item.TestId }, null)
        </td>
        <td>
            @Html.DisplayFor(model => item.StartDateTime)
        </td>
        <td>
            @Html.DisplayFor(model => item.EndDateTime)
        </td>
        <td>
            @Html.DisplayFor(model => item.TotalCorrectAnswers)
        </td>
        <td>
            @Html.DisplayFor(model => item.CorrectAnswers)
        </td>
        <td>
            @Html.DisplayFor(model => item.CorrectPercent)%
        </td>
    </tr>
}